/dist/**/*
/tmp
/out-tsc
/bazel-out

# Allow the app-host folder
!/dist/app-host

# Allow the browser folder inside app-host
!/dist/app-host/browser

# Allow the specific files
!/dist/app-host/browser/polyfills.js
!/dist/app-host/browser/main.js

# Node
/node_modules
npm-debug.log
yarn-error.log

